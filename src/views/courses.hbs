<ul class='courses'>
  {{#each courses}}
    <div class='card' style='width: 18rem;'>
      <img src='{{this.thumbnail}}' class='card-img-top' alt='...' />
      <div class='card-body'>
        <h5 class='card-title'>
					{{this.name}}
				</h5>
        <p class='card-text'>{{this.desc}}</p>
        <a href='#' class='btn btn-primary'>Go somewhere</a>
      </div>
    </div>
  {{/each}}
</ul>